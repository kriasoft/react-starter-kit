{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "composite": true,
    "emitDeclarationOnly": true,
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "noEmit": false,
    "outDir": "../.cache/ts-server",
    "types": ["node", "vite/client", "bun-types"]
  },
  "include": ["**/*.ts", "**/*.json"],
  "exclude": ["**/dist/**/*", "**/node_modules/**/*"],
  "references": [{ "path": "../db" }]
}
