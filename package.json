{
  "name": "root",
  "version": "0.0.0",
  "private": true,
  "packageManager": "yarn@4.0.0-rc.14",
  "type": "module",
  "workspaces": [
    "app"
  ],
  "scripts": {
    "postinstall": "husky install && yarn node ./scripts/postinstall.js",
    "start": "webpack serve --mode=development",
    "update-schema": "zx ./scripts/update-schema.js",
    "lint": "eslint --cache --report-unused-disable-directives .",
    "test": "jest",
    "build": "webpack --mode=production",
    "cf": "zx ./scripts/cf.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.9",
    "@babel/core": "^7.18.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.9",
    "@babel/plugin-transform-runtime": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/register": "^7.18.9",
    "@emotion/eslint-plugin": "^11.7.0",
    "@jest/types": "^28.1.3",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@types/babel__core": "^7.1.19",
    "@types/babel__plugin-transform-runtime": "^7.9.2",
    "@types/babel__preset-env": "^7.9.2",
    "@types/eslint": "^8.4.5",
    "@types/prettier": "^2.6.3",
    "@types/webpack-bundle-analyzer": "^4.4.1",
    "@types/webpack-env": "^1.17.0",
    "@types/webpack-manifest-plugin": "^3.0.5",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "babel-jest": "^28.1.3",
    "babel-loader": "^8.2.5",
    "babel-plugin-import": "^1.13.5",
    "copy-webpack-plugin": "^11.0.0",
    "cross-spawn": "^7.0.3",
    "del-cli": "^5.0.0",
    "envars": "^0.4.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "execa": "^6.1.0",
    "got": "^12.2.0",
    "graphql": "^16.5.0",
    "html-webpack-plugin": "^5.5.0",
    "http-proxy-middleware": "^2.0.6",
    "husky": "^8.0.1",
    "ignore-webpack-plugin": "^0.3.0",
    "inline-chunk-html-plugin": "^1.1.1",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "react-refresh": "^0.14.0",
    "relay-config": "^12.0.1",
    "terser-webpack-plugin": "^5.3.3",
    "type-fest": "^2.17.0",
    "typescript": "^4.7.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3",
    "webpack-manifest-plugin": "^5.0.0",
    "wrangler": "^2.0.23",
    "zx": "^7.0.7"
  },
  "resolutions": {
    "chalk@npm:^5.0.1": "^4.1.2"
  },
  "sideEffects": false,
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "cacheDirectory": ".cache/jest",
    "testPathIgnorePatterns": [
      "<rootDir>/.cache/",
      "<rootDir>/.github/",
      "<rootDir>/.husky/",
      "<rootDir>/.vscode/",
      "<rootDir>/.yarn/",
      "<rootDir>/dist/",
      "<rootDir>/scripts/"
    ]
  },
  "relay": {
    "src": ".",
    "schema": "schema.graphql",
    "language": "typescript",
    "artifactDirectory": "./queries",
    "schemaExtensions": [
      "."
    ],
    "eagerEsModules": true
  },
  "envars": {
    "cwd": "./core"
  }
}
