    "chai": "^4.0.2",
    "chokidar": "^1.7.0",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "enzyme": "^2.9.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-import-resolver-node": "^0.3.1",
    "eslint-loader": "^1.8.0",
    "eslint-plugin-css-modules": "^2.7.1",
    "eslint-plugin-flowtype": "^2.34.1",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "file-loader": "^0.11.2",
    "front-matter": "^2.1.2",
    "glob": "^7.1.2",
    "husky": "^0.14.1",
    "lint-staged": "^4.0.0",
    "markdown-it": "^8.3.1",
    "mkdirp": "^0.5.1",
    "mocha": "^3.4.2",
    "null-loader": "^0.1.1",
