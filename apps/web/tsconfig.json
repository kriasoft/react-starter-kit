{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "composite": true,
    "noEmit": true,
    "tsBuildInfoFile": "../../.cache/tsconfig/web.tsbuildinfo",
    "baseUrl": ".",
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "paths": {
      "@/*": ["./*"],
      "@repo/ui": ["../../packages/ui"],
      "@repo/ui/*": ["../../packages/ui/*"]
    },
    "types": ["astro/client", "@cloudflare/workers-types"]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.json",
    "**/*.astro",
    "types/**/*.d.ts"
  ],
  "exclude": ["**/dist/**/*", "**/node_modules/**/*"],
  "references": [{ "path": "../api" }, { "path": "../../packages/ui" }]
}
