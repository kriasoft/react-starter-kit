{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "types": ["vite/client"],
    "outDir": "../.cache/ts-app",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", "**/*.json", "./global.d.ts"],
  "exclude": ["**/dist/**/*", "**/node_modules/**/*", "lib/routeTree.gen.ts"]
}
