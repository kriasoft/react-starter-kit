{
  "editor.codeActionsOnSave": {
    "source.organizeImports": true
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "eslint.nodePath": ".yarn/sdks",
  "eslint.runtime": "node",
  "eslint.nodeEnv": "EDITOR",
  "prettier.prettierPath": ".yarn/sdks/prettier/index.js",
  "typescript.tsdk": ".yarn/sdks/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "jest.jestCommandLine": "yarn node --experimental-vm-modules $(yarn bin jest)",
  "jestrunner.jestCommand": "yarn node --experimental-vm-modules $(yarn bin jest)",
  "jestrunner.enableYarnPnpSupport": true,
  "jestrunner.yarnPnpCommand": "yarn-4.0.0-rc.14.cjs",
  "jestrunner.debugOptions": {
    "program": "yarn node $(yarn bin jest)",
    "env": {
      "NODE_OPTIONS": "--experimental-vm-modules"
    }
  },
  "files.exclude": {
    "**/.cache": true,
    "**/.DS_Store": true,
    "**/.editorconfig": true,
    "**/.eslintcache": true,
    "**/.git": true,
    "**/.gitattributes": true,
    "**/.husky": true,
    "**/.pnp.*": true,
    "**/.prettierignore": true,
    "**/node_modules": true,
    "**/yarn.lock": true
  },
  "search.exclude": {
    "yarn-error.log": true,
    "yarn.lock": true,
    "**/.yarn": true,
    "**/.pnp.*": true
  },
  "cSpell.ignoreWords": [
    "browserslist",
    "cloudfunctions",
    "corejs",
    "endregion",
    "entrypoint",
    "envalid",
    "envars",
    "eslintcache",
    "esmodules",
    "esnext",
    "execa",
    "firestore",
    "globby",
    "kriasoft",
    "miniflare",
    "oidc",
    "pathinfo",
    "pnpify",
    "reactstarter",
    "refetch",
    "refetchable",
    "sourcemap",
    "spdx",
    "tslib",
    "typechecking",
    "webflow",
    "yarnpkg",
    "yarnrc"
  ]
}
